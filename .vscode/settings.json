{
  "black-formatter.args": [
    "--line-length",
    "88",
    "--skip-magic-trailing-comma"
  ],
  "conventionalCommits.scopes": ["readme"],
  "isort.args": ["--profile", "black"],
  "pylint.args": ["--rc-file=.pylintrc"],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  }
}
